{% block html_head %}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!--><html class="no-js no-svg" {{ site.language_attributes }}> <!--<![endif]-->
<head>
    <title>{{site.name}}</title>
    <meta charset="{{ site.charset }}" />
    <meta name="description" content="{{ site.description }}">
    <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="author" href="{{ site.theme.link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="profile" href="http://gmpg.org/xfn/11">
    {{function('wp_head')}}

    <link href="https://fonts.googleapis.com/css?family=Lato:300i,400|Montserrat:300,400,400i,700,700i&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/d9f5bf3c71.js" crossorigin="anonymous"></script>
{% endblock %}

	<body class="{{body_class}}">
		<header class="header" >
			{% include('partial/header.html.twig') %}
		</header>

		<section id="content" role="main" class="content-wrapper">
			{% block body %}
                {{ post.content }}
			{% endblock %}
		</section>

		{% block footer %}
			{% include('partial/footer.html.twig') %}
            {{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>