        "gulp-concat": "2.5.2",
        "gulp-plumber": "1.0.0",
        "gulp-rename": "1.2.2",
        "gulp-sass": "3.0.0",
        "gulp-uglify": "1.2.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0"
    },
    "main": "gulpfile.js"
